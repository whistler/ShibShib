<div class="tabbable" style="margin-bottom: 9px;">
  <ul class="nav nav-tabs">
    <li class="active"><a href="<%= index_path %>"><%= t 'tabs.latest' %></a></li>
    <li><a href="<%= mvote_path %>"><%= t 'tabs.most_voted' %></a></li>
  </ul>
  <div class="tab-content">
    <div class="tab-pane active" id="0">
      <div id="posts">
      <%= render @posts %>
      </div>
    </div>
  </div> <!-- /tab-content -->
</div> <!-- /tabbable -->
<%= will_paginate(@posts, :more => true) %>
